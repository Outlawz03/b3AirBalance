 */
Multi = {
	init: function(node) {
		this.states = [];
		this.content = "---";
		
		if(node) {
			this.text = node.getChildByName("ValueText");
				
			i = 1;
			while(node.hasAttribute("State" + i)) {
				this.states[i] = String(node.getAttribute("State" + i));
				i += 1;
			}
