/**==========================================================================================
 * Text Object
 */
Text = {
	init: function(node) {
		this.content = "---";
		
		if(node) {
			this.text = node.getChildByName("ValueText");
		}		
	},
	update: function(val) {
		this.content = val;
		this.render();
